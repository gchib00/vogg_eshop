ses.appBar} color='inherit'>\n                <Toolbar>\n                    <Typography component={Link} to='/'>\n                        <img src={logo} alt=\"Vogg\" height=\"100px\" className={classes.image}/>\n                    </Typography>\n\n                    <div className={classes.grow} />\n\n                    {location.pathname !== '/cart' ? (  // if user is on the cart page, they won't be show the cart icon\n                    <div>\n                        <IconButton component={Link} to='/cart' aria-label='Show cart items' color='grey'>\n                            <Badge badgeContent={totalItems} color=\"secondary\">\n                                <ShoppingCart />\n                            </Badge>\n                        </IconButton>\n                    </div>\n                    ) : null }\n                </Toolbar>\n            </AppBar>\n        </div>\n    )\n}\n\nexport default Navbar","/home/gchib00/Desktop/vogg_eshop/src/components/Products/Products.jsx",["84"],"import React, { useState } from 'react'\nimport { Grid, Button } from '@material-ui/core'\nimport Product from './Product/Product'\nimport useStyles from \"./styles\"\n\nfunction Products({products, onAddToCart}) {\n\n    const [gender, setGender] = useState('All')\n    const classes = useStyles()\n\n    let femaleShoes = []; let maleShoes = [];\n    let renderArray = products\n\n    // Categorizes shoes according to gender\n    for (let i=0; i<products.length; i++) { \n        let eachName = products[i].name\n        if (eachName.includes('M')){\n            maleShoes.push(products[i])\n        }\n        if (eachName.includes('W')){\n            femaleShoes.push(products[i])\n        }\n    } \n    ////////////////////////////////////////\n\n    // decides which array (male, female or both) should be rendered\n    if (gender === \"all\"){\n        renderArray = products\n    }\n    if (gender === \"Female\"){\n        renderArray = femaleShoes\n    }\n    if (gender === \"Male\"){\n        renderArray = maleShoes\n    }\n    //////////////////////////////////////////////////////////////////\n    \n    // lets user render products specific to only one gender \n    function handleChange(event) {\n        setGender(event.target.value)\n    }\n    ////////////////////////////////////////////////////////\n    return(\n        <main className={classes.content} >\n            <div className={classes.toolbar} />\n            <div className={classes.genderfilterContainer}>\n                <div className={classes.genderFilter}>\n                    <label className={classes.option1} style={{fontFamily: 'Roboto, sans-serif'}}>\n                        <input \n                            type='radio' \n                            name='genderRadioGroup' \n                            value='All'  \n                            checked={gender === 'All'} \n                            id='test1'\n                            onChange={handleChange}\n                            style={{display: 'none'}}\n                            />ყველა\n                    </label>\n                    <label className={classes.option2} style={{fontFamily: 'Roboto, sans-serif'}}>\n                        <input \n                            type='radio' \n                            name='genderRadioGroup' \n                            value='Female' \n                            checked={gender === 'Female'}\n                            onChange={handleChange}    \n                            style={{display: 'none'}}\n                            />ქალი\n                    </label>\n                    <label className={classes.option3} style={{fontFamily: 'Roboto, sans-serif'}}>\n                        <input \n                            type='radio' \n                            name='genderRadioGroup' \n                            value='Male' \n                            checked={gender === 'Male'} \n                            onChange={handleChange} \n                            style={{display: 'none'}}  \n                            />კაცი\n                    </label>\n                </div>\n            </div>\n            <Grid container justify=\"center\" spacing={4}>\n                {renderArray.map((product) => (\n                    <Grid item key={product.id} xs={12} sm={6} md={4} lg={3}>\n                        <Product product={product} onAddToCart={onAddToCart} />\n                    </Grid>  \n                ))}\n            </Grid>\n        </main>\n    )\n\n}\n\n\nexport default Products","/home/gchib00/Desktop/vogg_eshop/src/components/Products/Product/Product.jsx",[],"/home/gchib00/Desktop/vogg_eshop/src/components/Products/Product/styles.js",[],"/home/gchib00/Desktop/vogg_eshop/src/components/Navbar/styles.js",[],"/home/gchib00/Desktop/vogg_eshop/src/components/Products/styles.js",[],"/home/gchib00/Desktop/vogg_eshop/src/lib/commerce.js",[],"/home/gchib00/Desktop/vogg_eshop/src/components/Cart/Cart.jsx",[],"/home/gchib00/Desktop/vogg_eshop/src/components/Cart/styles.js",[],"/home/gchib00/Desktop/vogg_eshop/src/components/Cart/CartItem/CartItem.jsx",[],"/home/gchib00/Desktop/vogg_eshop/src/components/Cart/CartItem/styles.js",[],"/home/gchib00/Desktop/vogg_eshop/src/components/CheckoutForm/Checkout/Checkout.jsx",["85","86","87","88"],"/home/gchib00/Desktop/vogg_eshop/src/components/CheckoutForm/Checkout/styles.js",[],"/home/gchib00/Desktop/vogg_eshop/src/components/CheckoutForm/PaymentForm.jsx",["89","90","91","92","93","94","95"],"/home/gchib00/Desktop/vogg_eshop/src/components/CheckoutForm/AddressForm.jsx",["96","97","98","99"],"/home/gchib00/Desktop/vogg_eshop/src/components/CheckoutForm/FormInput.jsx",[],"/home/gchib00/Desktop/vogg_eshop/src/components/CheckoutForm/Review.jsx",["100","101","102"],{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","severity":1,"message":"108","line":2,"column":22,"nodeType":"109","messageId":"110","endLine":2,"end