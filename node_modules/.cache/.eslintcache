s\n    }\n    if (gender === \"Male\"){\n        renderArray = maleShoes\n    }\n    //////////////////////////////////////////////////////////////////\n    \n    // lets user render products specific to only one gender \n    function handleChange(event) {\n        setGender(event.target.value)\n    }\n    ////////////////////////////////////////////////////////\n    return(\n        <main className={classes.content} >\n            <div className={classes.toolbar} />\n            <div className={classes.genderfilterContainer}>\n                <div className={classes.genderFilter}>\n                    <label className={classes.option1} style={{fontFamily: 'Roboto, sans-serif'}}>\n                        <input \n                            type='radio' \n                            name='genderRadioGroup' \n                            value='All'  \n                            checked={gender === 'All'} \n                            onChange={handleChange}\n                            style={{display: 'none'}}\n                            />ყველა\n                    </label>\n                    <label className={classes.option2} style={{fontFamily: 'Roboto, sans-serif'}}>\n                        <input \n                            type='radio' \n                            name='genderRadioGroup' \n                            value='Female' \n                            checked={gender === 'Female'}\n                            onChange={handleChange}    \n                            style={{display: 'none'}}\n                            />ქალი\n                    </label>\n                    <label className={classes.option3} style={{fontFamily: 'Roboto, sans-serif'}}>\n                        <input \n                            type='radio' \n                            name='genderRadioGroup' \n                            value='Male' \n                            checked={gender === 'Male'} \n                            onChange={handleChange} \n                            style={{display: 'none'}}  \n                            />კაცი\n                    </label>\n                </div>\n            </div>\n            <Grid container\n                style={{ \n                    display: 'flex', \n                    justifyContent: 'center', \n                    alignItems: 'center',\n                    margin: '0px',\n                    boxSizing: 'border-box',\n                }}>\n                {renderArray.map((product) => (\n                    <Grid item key={product.id} xs={12} sm={5} md={3} lg={2} style={{ margin: '2rem' }}>\n                        <Product product={product} onAddToCart={onAddToCart} />\n                    </Grid>  \n                ))}\n            </Grid>\n        </main>\n    )\n\n}\n\n\nexport default Products","/home/gchib00/Desktop/vogg_eshop/src/components/Footer/Footer.jsx",["54"],"/home/gchib00/Desktop/vogg_eshop/src/components/Navbar/styles.js",[],"/home/gchib00/Desktop/vogg_eshop/src/components/Products/styles.js",[],"/home/gchib00/Desktop/vogg_eshop/src/components/Products/Product/Product.jsx",[],"/home/gchib00/Desktop/vogg_eshop/src/components/Products/Product/styles.js",[],{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","severity":1,"message":"60","line":3,"column":27,"nodeType":"61","messageId":"62","endLine":3,"endColumn":33},{"ruleId":"59","severity":1,"message":"63","line":3,"column":35,"nodeType":"61","messageId":"62","endLine":3,"endColumn":41},{"ruleId":"59","severity":1,"message":"64","line":3,"column":43,"nodeType":"61","messageId":"62","endLine":3,"endColumn":48},{"ruleId":"59","severity":1,"message":"65","line":2,"column":22,"nodeType":"61","messageId":"62","endLine":2,"endColumn":28},{"ruleId":"59","severity":1,"message":"66","line":2,"column":30,"nodeType":"61","messageId":"62","endLine":2,"endColumn":35},{"ruleId":"59","severity":1,"message":"65","line":2,"column":16,"nodeType":"61","messageId":"62","endLine":2,"end